{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/**/*", "src/**/*.vue", "env.d.ts"],
  "exclude": ["src/**/__tests__/*", "src/constants/**/*", "src/utils/**/*"],
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "noImplicitAny": false,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
