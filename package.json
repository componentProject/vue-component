{
  "name": "vue-template",
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "author": "1983531544@qq.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moluoxixi-template/vue-template.git"
  },
  "keywords": [
    "electron",
    "nest",
    "elementPlus",
    "vue3",
    "typescript",
    "vite",
    "pinia",
    "template"
  ],
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "CICD": "node ./scripts/CICD.mjs",
    "build:old": "run-p type-check \"build-only {@}\" -- & node scripts/build.mjs",
    "build": "vite build --mode production & node ./scripts/build.mjs",
    "build:cdn": "vite build --mode production & node ./scripts/build.mjs",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "commit": "git-cz"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@sentry/vite-plugin": "^3.5.0",
    "@sentry/vue": "^9.27.0",
    "axios": "^1.9.0",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "pinia": "2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "radash": "^12.1.0",
    "rxjs": "^7.8.1",
    "vue": "^3.5.13",
    "vue-router": "4.5.0",
    "vxe-pc-ui": "4.6.21",
    "vxe-table": "4.13.39"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@tsconfig/node22": "^22.0.1",
    "@types/lodash": "^4.17.17",
    "@types/node": "^22.14.0",
    "@types/rollup": "^0.54.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "compressing": "^1.10.1",
    "cz-customizable": "^7.4.0",
    "dotenv": "^16.5.0",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^15.5.1",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.87.0",
    "sass-embedded": "^1.87.0",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^4.1.5",
    "typescript": "~5.8.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.7.0",
    "vite": "^6.2.4",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-qiankun": "^1.0.15",
    "vite-plugin-resolve-externals": "^0.2.2",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  },
  "prettier": {
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
}
