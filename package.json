  "prettier": {
    "$schema": "https://json.schemastore.org/prettierrc",
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "lint-staged": {
    "!src/components/ConfigForm/*": [],
    "!src/components/ConfigForm/**/*": [],
    "!src/components/DragTable/**/*": [],
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json,md}": [
      "prettier --write"
    ],
    "*.{css,scss,less,vue}": [
      "stylelint --fix"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
