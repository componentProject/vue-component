{
  "name": "vuecomponent",
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "author": "1983531544@qq.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moluoxixi/vue-component.git"
  },
  "keywords": [
    "electron",
    "nest",
    "elementPlus",
    "vue3",
    "typescript",
    "vite",
    "pinia",
    "template"
  ],
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "CICD": "node ./scripts/CICD.mjs",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --write src/components/**/*.{js,jsx,ts,tsx,css,less,scss,vue}",
    "lint:style": "stylelint --fix src/components/**/*.{css,less,scss,vue}",
    "lint:eslint": "eslint --fix src/components/**/*.{js,jsx,ts,tsx,vue}",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "prepare": "husky",
    "storybook:dev": "set VITE_USE_CDN=false && storybook dev -p 6006",
    "storybook:build": "set VITE_USE_CDN=false && storybook build -o docs/storybook",
    "vitepress:dev": "vitepress dev .docs",
    "vitepress:build": "vitepress build .docs",
    "vitepress:preview": "vitepress preview .docs"
  },
